{
  "name": "pwa-starter",
  "version": "0.0.1",
  "description": "A starter kit for building PWAs!",
  "main": "index.js",
  "scripts": {
    "build": "tsc && vite build",
    "start": "vite --open --port 4200",
    "start-remote": "vite --open --port 4200 --host",
    "fix": "prettier --write .",
    "build-demo": "tsc && vite build && rm dist/data/ootw.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@free-side/audioworklet-polyfill": "^1.1.3-beta.4",
    "@pwabuilder/pwainstall": "^1.6.7",
    "@shoelace-style/shoelace": "^2.0.0-beta.82",
    "@vaadin/router": "^1.7.4",
    "fflate": "^0.7.4",
    "gamecontroller.js": "^1.5.0",
    "lit": "^2.3.1",
    "stats.js": "^0.17.0",
    "workbox-build": "^6.5.2",
    "workbox-core": "^6.5.2",
    "workbox-precaching": "^6.5.2"
  },
  "devDependencies": {
    "@types/stats.js": "^0.17.0",
    "gh-pages": "^5.0.0",
    "netlify-cli": "^13.0.1",
    "prettier": "^2.8.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.0",
    "vite-plugin-pwa": "^0.11.13"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "es5",
    "endOfLine": "crlf",
    "bracketSpacing": true
  }
}
